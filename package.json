{
  "name": "tmpl-react-exp-gcp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "react-scripts start",
    "build": "concurrently \"react-scripts build\" \"npm-watch build\"",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start": "node index.js",
    "mount": "nodemon --ignore src/ --ignore public/ --ignore build/ index.js",
    "dev": "concurrently \"npm run mount\" \"npm run serve\" \"npm run build\"",
    "deploy": "gcloud app deploy"
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 100,
    "trailingComma": "es5"
  },
  "proxy": "http://localhost:8888",
  "watch": {
    "build": {
      "patterns": [
        "src",
        "public"
      ],
      "extensions": [
        "*"
      ],
      "quiet": true,
      "runOnChangeOnly": true
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@google-cloud/logging": "^7.3.0",
    "@reduxjs/toolkit": "^1.3.4",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "axios": "^0.21.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.3",
    "express": "^4.17.1",
    "node-sass": "^4.13.1",
    "npm-watch": "^0.6.0",
    "querystring": "^0.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "nodemon": "^2.0.3",
    "prettier": "2.0.4",
    "redux-devtools": "^3.5.0"
  }
}